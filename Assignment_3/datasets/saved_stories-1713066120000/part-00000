43e\u043c \u043a\u043e\u043d\u0446\u0435 \u0443\u0441\u0440\u0435\u0434\u043d\u044f\u0435\u043c \u043e\u0442\u0432\u0435\u0442\u044b.\n\n\u0414\u0430\u043d\u043d\u044b\u0435 \u0432 \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u044b\u0445 \u0432\u044b\u0431\u043e\u0440\u043a\u0430\u0445 \u043c\u043e\u0433\u0443\u0442 \u043f\u043e\u0432\u0442\u043e\u0440\u044f\u0442\u044c\u0441\u044f. \u0422\u043e \u0435\u0441\u0442\u044c \u0438\u0437 \u043d\u0430\u0431\u043e\u0440\u0430 1-2-3 \u043c\u044b\n\u043c\u043e\u0436\u0435\u043c \u0434\u0435\u043b\u0430\u0442\u044c \u0432\u044b\u0431\u043e\u0440\u043a\u0438 2-2-3, 1-2-2, 3-1-2 \u0438 \u0442\u0430\u043a \u043f\u043e\u043a\u0430 \u043d\u0435 \u043d\u0430\u0434\u043e\u0435\u0441\u0442. \u041d\u0430 \u043d\u0438\u0445 \u043c\u044b\n\u043e\u0431\u0443\u0447\u0430\u0435\u043c \u043e\u0434\u0438\u043d \u0438 \u0442\u043e\u0442 \u0436\u0435 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0437, \u0430 \u0432 \u043a\u043e\u043d\u0446\u0435 \u0432\u044b\u0447\u0438\u0441\u043b\u044f\u0435\u043c \u043e\u0442\u0432\u0435\u0442\n\u043f\u0440\u043e\u0441\u0442\u044b\u043c \u0433\u043e\u043b\u043e\u0441\u043e\u0432\u0430\u043d\u0438\u0435\u043c.\n\n\u0421\u0430\u043c\u044b\u0439 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0439 \u043f\u0440\u0438\u043c\u0435\u0440 \u0431\u0435\u0433\u0433\u0438\u043d\u0433\u0430 \u2014 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c Random Forest, \u0431\u0435\u0433\u0433\u0438\u043d\u0433 \u043d\u0430\n\u0434\u0435\u0440\u0435\u0432\u044c\u044f\u0445, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0438 \u043d\u0430\u0440\u0438\u0441\u043e\u0432\u0430\u043d \u043d\u0430 \u043a\u0430\u0440\u0442\u0438\u043d\u043a\u0435. \u041a\u043e\u0433\u0434\u0430 \u0432\u044b \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u0435\u0442\u0435 \u043a\u0430\u043c\u0435\u0440\u0443 \u043d\u0430\n\u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0435 \u0438 \u04